            WFMediaBox.init({
                "base": "\/",
                "theme": "standard",
                "mediafallback": 0,
                "mediaselector": "audio,video",
                "width": "",
                "height": "",
                "lightbox": 0,
                "shadowbox": 0,
                "icons": 1,
                "overlay": 1,
                "overlay_opacity": 0.8,
                "overlay_color": "#000000",
                "transition_speed": 500,
                "close": 2,
                "scrolling": "fixed",
                "labels": {
                    "close": "Close",
